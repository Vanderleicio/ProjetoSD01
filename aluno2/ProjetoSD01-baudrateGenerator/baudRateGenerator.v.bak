module baudRateGenerator(clk_in, reset, out);
	input clk_in;
	input reset;	
	
	reg [9:0]baud_Rate = 10'b101000101100;
	reg [9:0]baudRateReg; 
		
	
	output out;
	
	always @(posedge clk_in or negedge reset)
		 if (!reset) baudRateReg <= 10'b1;
		 
		 else if (out) baudRateReg <= 10'b1;
				else baudRateReg <= baudRateReg + 1'b1;
				
	assign out = (baudRateReg == baud_Rate);


	
endmodule
