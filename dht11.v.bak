// Modulo DHT11

module DHT11 (
	input clk, 
	input en,
	input rst,
	inout dht_data, //Pino em tri-state
	output [7:0] hum_int,
	output [7:0] hum_float,
	output [7:0] temp_int,
	output [7:0] temp_float,
	output [7:0] crc,
	output espera, //Informa quando o circuito está operando e aguardando algum retorno do dht11
	);
	
	reg dht_out, dir, wait_reg, debug_reg; // Registradores de saída
	reg [25:0] counter; //Contador de ciclos para gerar delays
	reg [5:0] index;
	reg [39:0] intdata; // Armazenar as informações que serão obtidas do dht11
	reg dht_in;
	
	assign wait = wait_reg;
	
	
	